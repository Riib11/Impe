fib(n: int): int = {
  if n = 0 then
    return 0; 
  else if n = 1 then
    return 1;
  else 
    return fib(n - 1) + fib(n - 2);
}

main(): void = {
  i_start: int <- 0;
  i_end: int <- 15;

  i: int <- i_start;
  while i < i_end do {
    write_string(int_to_string(fib(i)) <> "\n");
    i <- i + 1;
  }
}