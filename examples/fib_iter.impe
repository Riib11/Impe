fib(n: int): int = {
  i: int;
  i <- 0;

  r: int;
  r <- 0; 

  while <=(i, n) do {
    r <- +(r, i);
    i <- +(i, 1);
  }

  return r;
}

main(): void = {
  i_start: int;
  i_end: int; 
  
  i_start <- 0;
  i_end <- 20;

  i: int;
  i <- i_start;
  while <(i, i_end) do {
    write_int(fib(i));
    write_string("\n");
    i <- +(i, 1);
  }
}