fib(n: int): int = {
  if n = 0 then return 0; else pass;
  r1: int;
  r1 <- 0;

  if n = 1 then return 1; else pass;
  r2: int;
  r2 <- 1;

  i: int;
  i <- 1;
  while i < n do {
    x: int;
    x <- r1 + r2;

    r1 <- r2;
    r2 <- x;

    i <- i + 1;
  }

  return r2;
}

main(): void = {
  i_start: int;
  i_end: int; 
  
  i_start <- 0;
  i_end <- 15;

  i: int;
  i <- i_start;
  while i < i_end do {
    write_string(int_to_string(fib(i)) <> "\n");
    i <- i + 1;
  }
}